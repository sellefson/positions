<div class="page-header">
  <h1>
    Electedofficials
    <a href="/electedofficials/new" class="btn btn-lg btn-success">New Electedofficial</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @electedofficials.each do |electedofficial| %>
      <tr>
        <td><%= electedofficial.name %></td>
        <td>
          <a href="/electedofficials/<%= electedofficial.id %>" class="btn btn-primary">Show</a>
          <a href="/electedofficials/<%= electedofficial.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_electedofficial/<%= electedofficial.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @electedofficials, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
